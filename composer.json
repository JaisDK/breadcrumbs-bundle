{
    "name": "yceruto/breadcrumbs-bundle",
    "type": "symfony-bundle",
    "description": "breadcrumbs for symfony application",
    "keywords": [
        "breadcrumbs"
    ],
    "homepage": "https://github.com/yceruto/breadscrumbs-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Yonel Ceruto",
            "email": "yonelceruto@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/http-kernel": "~2.3|~3.0",
        "symfony/http-foundation": "~2.3|~3.0",
        "symfony/dependency-injection": "~2.3|~3.0",
        "symfony/routing": "~2.6,>2.6.4",
        "symfony/config": "~2.3|~3.0",
        "symfony/framework-bundle": "~2.3|~3.0",
        "twig/extensions": "~1.0",
        "twig/twig": "~1.14,>=1.14.2|~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4",
        "symfony/phpunit-bridge": "^2.7",
        "symfony/expression-language": "~2.6"
    },
    "autoload": {
        "psr-4": {
            "Yceruto\\Bundle\\BreadcrumbsBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}

